    <div class="mb-4">
      <div class="row d-flex justify-content-start flex-wrap">
        <%= render "common/attribute_input", f:f, attribute: :template_rdv_title_override, value: configuration.template_rdv_title_override || template.rdv_title %>
        <%= render "common/attribute_input", f:f, attribute: :template_rdv_title_by_phone_override, value: configuration.template_rdv_title_by_phone_override || template.rdv_title_by_phone %>
        <%= render "common/attribute_input", f:f, attribute: :template_applicant_designation_override, value: configuration.template_applicant_designation_override || template.applicant_designation %>
        <%= render "common/attribute_input", f:f, attribute: :template_rdv_purpose_override, value: configuration.template_rdv_purpose_override || template.rdv_purpose %>
      </div>
    </div>
  </div>
