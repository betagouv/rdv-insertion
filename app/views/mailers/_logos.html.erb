<% if display_europe_logos %>
  <%= image_tag "logos/europe-logos.jpg", alt: "Logo de l'Union européene", class: "structure-logo" if display_europe_logos %>
<% end %>
<% if organisation_logo %>
  <%= image_tag organisation_logo, alt: "Logo de l'organisation", class: "structure-logo" %>
<% end %>
<% if department_logo && display_department_logo %>
  <%= image_tag department_logo, alt: "Logo du département", class: "structure-logo" %>
<% end %>
<% if display_france_travail_logo %>
  <%= image_tag "logos/france-travail.png", alt: "Logo de France Travail", class: "structure-logo" %>
<% end %>
